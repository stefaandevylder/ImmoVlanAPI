<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:element name="request" type="request" />

  <xs:complexType name="request">
    <xs:sequence>
      <xs:element name="action" type="action" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="timestamp" type="xs:dateTime" use="required"/>
    <xs:attribute name="softwareId" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:int">
          <xs:minInclusive value="1" />
          <xs:maxInclusive value="1000" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name="action">
    <xs:sequence>
      <xs:element name="publish" type="publish" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="create" type="create" minOccurs="0" maxOccurs="1" />
      <xs:element name="suspend" type="suspend" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="proCustomerId" type="xs:string" use="required"/>
    <xs:attribute name="hashValidation" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="publish">
    <xs:sequence>
      <xs:element name="batch" type="batch" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="project" type="project" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="property" type="property" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="create">
    <xs:sequence>
      <xs:element name="upsell" minOccurs="0" maxOccurs="20" type="upsell" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="suspend">
    <xs:sequence>
      <xs:element name="property" minOccurs="0" maxOccurs="unbounded" type="propertySuspend"/>
      <xs:element name="batch" minOccurs="0" maxOccurs="unbounded" type="batchSuspend"/>
      <xs:element name="upsell" minOccurs="0" maxOccurs="unbounded" type="upsell" />
      <xs:element name="project" minOccurs="0" maxOccurs="unbounded" type="projectSuspend" />
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="property">
    <xs:sequence>
      <xs:element name="classification" minOccurs="1" maxOccurs="1" type="classification" />
      <xs:element name="location" minOccurs="1" maxOccurs="1" type="location"/>
      <xs:element name="generalInformation" minOccurs="0" maxOccurs="1" type="generalInformation" />
      <xs:element name="freeDescription" minOccurs="1" maxOccurs="1" type="freeDescription"/>
      <xs:element name="financialDetails" minOccurs="1" maxOccurs="1" type="financialDetails"/>
      <xs:element name="outdoorDescription" minOccurs="0" maxOccurs="1" type="outdoorDescription" />
      <xs:element name="indoorDescription" minOccurs="0" maxOccurs="1" type="indoorDescription"/>
      <xs:element name="environment" minOccurs="0" maxOccurs="1" type="environment" />
      <xs:element name="buildingRequirements" minOccurs="0" maxOccurs="1" type="buildingRequirements"/>
      <xs:element name="certificates" minOccurs="0" maxOccurs="1" type="certificates" />
      <xs:element name="propertyEquipment" minOccurs="0" maxOccurs="1" type="propertyEquipment"/>
      <xs:element name="businessSpace" minOccurs="0" maxOccurs="1" type="businessSpace"/>
      <xs:element name="returnBuilding" minOccurs="0" maxOccurs="1" type="returnBuilding"/>
      <xs:element name="garage" minOccurs="0" maxOccurs="1" type="garage" />
      <xs:element name="studentRoom" minOccurs="0" maxOccurs="1" type="studentRoom"/>
      <xs:element name="roommate" minOccurs="0" maxOccurs="1" type="roommate"/>
      <xs:element name="lifeAnnuity" minOccurs="0" maxOccurs="1" type="lifeAnnuity"/>
      <xs:element name="publicAuction" minOccurs="0" maxOccurs="1" type="publicAuction" />
      <xs:element name="seasonalRental" minOccurs="0" maxOccurs="1" type="seasonalRental" />
      <xs:element name="attachments" minOccurs="0" maxOccurs="1" type="attachments" />
      <xs:element name="projectSubProperties" minOccurs="0" maxOccurs="1" type="projectSubProperties" />
    </xs:sequence>
    <xs:attribute name="propertyProId" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1"/>
		  <xs:maxLength value="100"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="propertySoftwareId"  use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="projectSoftwareId">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="batchSoftwareId">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="commercialStatus" type="commercialStatus" />
  </xs:complexType>

  <xs:simpleType name="commercialStatus">
    <xs:restriction base="xs:string">
      <xs:enumeration value="SOLD"/>
      <xs:enumeration value="OPTION"/>
      <xs:enumeration value="ONLINE"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="classification">
    <xs:sequence>
      <xs:element name="transactionType" minOccurs="1" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="RENT"/>
            <xs:enumeration value="SALE"/>
            <xs:enumeration value="SHARE"/>
            <xs:enumeration value="PUBLIC_SALE" />
            <xs:enumeration value="LIFE_ANNUITY" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="propertyTypeId" type="propertyTypeId" minOccurs="1" maxOccurs="1" />
      <xs:element name="isNewConstruction" type="xs:boolean" nillable="true" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="propertyTypeId">
    <xs:restriction base="xs:int">
      <xs:enumeration value="10"/>
      <xs:enumeration value="20"/>
      <xs:enumeration value="30"/>
      <xs:enumeration value="40"/>
      <xs:enumeration value="50"/>
      <xs:enumeration value="60"/>
      <xs:enumeration value="70"/>
      <xs:enumeration value="80"/>
      <xs:enumeration value="90"/>
      <xs:enumeration value="100"/>
      <xs:enumeration value="110"/>
      <xs:enumeration value="120"/>
      <xs:enumeration value="130"/>
      <xs:enumeration value="140"/>
      <xs:enumeration value="150"/>
      <xs:enumeration value="160"/>
      <xs:enumeration value="165"/>
      <xs:enumeration value="170"/>
      <xs:enumeration value="180"/>
      <xs:enumeration value="190"/>
      <xs:enumeration value="200"/>
      <xs:enumeration value="210"/>
      <xs:enumeration value="220"/>
      <xs:enumeration value="230"/>
      <xs:enumeration value="240"/>
      <xs:enumeration value="245"/>
      <xs:enumeration value="250"/>
      <xs:enumeration value="260"/>
      <xs:enumeration value="270"/>
      <xs:enumeration value="280"/>
      <xs:enumeration value="290"/>
      <xs:enumeration value="300"/>
      <xs:enumeration value="310"/>
      <xs:enumeration value="320"/>
      <xs:enumeration value="330"/>
      <xs:enumeration value="340"/>
      <xs:enumeration value="350"/>
      <xs:enumeration value="360"/>
      <xs:enumeration value="370"/>
      <xs:enumeration value="380"/>
      <xs:enumeration value="390"/>
      <xs:enumeration value="400"/>
      <xs:enumeration value="410"/>
      <xs:enumeration value="420"/>
      <xs:enumeration value="430"/>
      <xs:enumeration value="440"/>
      <xs:enumeration value="450"/>
      <xs:enumeration value="460"/>
      <xs:enumeration value="470"/>
      <xs:enumeration value="480"/>
      <xs:enumeration value="490"/>
      <xs:enumeration value="500"/>
      <xs:enumeration value="510"/>
      <xs:enumeration value="515"/>
      <xs:enumeration value="520"/>
      <xs:enumeration value="530"/>
      <xs:enumeration value="540"/>
      <xs:enumeration value="550"/>
      <xs:enumeration value="560"/>
      <xs:enumeration value="570"/>
      <xs:enumeration value="580"/>
      <xs:enumeration value="590"/>
      <xs:enumeration value="600"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="location">
    <xs:sequence>
      <xs:element name="address" type="address" minOccurs="1" maxOccurs="1" />
      <xs:element name="coordinates" type="coordinates" minOccurs="0" maxOccurs="1" />
      <xs:element name="isAddressDisplayed" type="xs:boolean" nillable="true" minOccurs="0" />
      <!--<xs:element name="timeZone" type="xs:string" minOccurs="0" />-->
      <xs:element name="environmentId" type="environmentId" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  
  <xs:complexType name="batchLocation">
    <xs:sequence>
      <xs:element name="address" type="address" minOccurs="1" maxOccurs="1" />
      <xs:element name="coordinates" type="coordinates" minOccurs="0" maxOccurs="1" />
      <!--<xs:element name="timeZone" type="xs:string" minOccurs="0" />-->
      <xs:element name="environmentId" type="environmentId" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="environmentId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="street">
    <xs:restriction base="xs:string">
      <xs:maxLength value="255" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="streetNumber">
    <xs:restriction base="xs:string">
      <xs:maxLength value="15" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="postBox">
    <xs:restriction base="xs:string">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="zipCode">
    <xs:restriction base="xs:string">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="city">
    <xs:restriction base="xs:string">
      <xs:maxLength value="100" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="province">
    <xs:restriction base="xs:string">
      <xs:maxLength value="50" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="country">
    <xs:restriction base="xs:string">
      <xs:maxLength value="255" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="address">
    <xs:sequence>
      <xs:element name="street" type="street" minOccurs="0" maxOccurs="1" />
      <xs:element name="streetNumber" type="streetNumber" minOccurs="0" maxOccurs="1" />
      <xs:element name="postBox" type="postBox" minOccurs="0" maxOccurs="1" />
      <xs:element name="zipCode" type="zipCode" minOccurs="1"  maxOccurs="1"/>
      <xs:element name="city" type="city" minOccurs="0" maxOccurs="1" />
      <xs:element name="province" type="province" minOccurs="0" maxOccurs="1" />
      <!--<xs:element name="region" type="xs:string" minOccurs="0" maxOccurs="1" />-->
      <xs:element name="country" type="country" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="coordinates">
    <xs:sequence>
      <xs:element name="latitude" type="xs:decimal" minOccurs="1" maxOccurs="1" />
      <xs:element name="longitude" type="xs:decimal" minOccurs="1" maxOccurs="1"  />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="generalInformation">
    <xs:sequence>
      <xs:element name="contactEmail" type="xs:string" minOccurs="0" />
      <xs:element name="contactPhone" type="xs:string" minOccurs="0" />
      <xs:element name="propertyUrl" type="xs:string" minOccurs="0" />
      <xs:element name="contactLanguage" type="xs:string" minOccurs="0" />
      <xs:element name="visitingHours" type="xs:string" minOccurs="0"  />
      <xs:element name="conditionId" type="conditionId" minOccurs="0" />
      <xs:element name="availabilityId" type="availabilityId" nillable="true"  minOccurs="0" />
      <xs:element name="availableFrom" minOccurs="0" type="xs:dateTime"/>
      <xs:element name="isFirstOccupation" type="xs:boolean" minOccurs="0" />
      <xs:element name="isCurrentlyRented" type="xs:boolean" minOccurs="0" />
      <xs:element name="buildYear" type="xs:int" minOccurs="0" />
      <xs:element name="cadastralIncome" type="xs:decimal" minOccurs="0" nillable="true" />
      <xs:element name="arePetsAllowed" type="xs:boolean" minOccurs="0" nillable="true" />
      <xs:element name="isWoodConstruction" type="xs:boolean" minOccurs="0"  nillable="true"  />
      <xs:element name="isListedBuilding" type="xs:boolean" minOccurs="0"  nillable="true"  />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="conditionId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="8" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="availabilityId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="freeDescription">
    <xs:sequence>
      <xs:element name="french" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="dutch" type="xs:string" minOccurs="1" maxOccurs="1" />
      <xs:element name="english" type="xs:string" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="financialDetails">
    <xs:sequence>
      <xs:element name="price" type="xs:decimal" />
      <xs:element name="maxPrice" type="xs:decimal" minOccurs="0" nillable="true"  />
      <xs:element name="pricePeriodicity" type="pricePeriodicityId" minOccurs="0" />
      <xs:element name="isPricePerSquareMeter" type="xs:boolean" minOccurs="0" nillable="true" />
      <xs:element name="currency" type="currency" minOccurs="0" />
      <xs:element name="leaseDuration" type="xs:int" minOccurs="0" maxOccurs="1" />
      <xs:element name="maintenanceCharges" type="xs:decimal" minOccurs="0" nillable="true"  />
      <xs:element name="rentalDeposit" type="xs:int" minOccurs="0" />
      <xs:element name="isVatApplied" type="xs:boolean" minOccurs="0" nillable="true" />
      <xs:element name="isPriceDisplayed" type="xs:boolean" minOccurs="0" nillable="true" />
      <xs:element name="priceTypeId" type="priceTypeId" />
      <xs:element name="monthlyRentalIncome" type="xs:decimal" minOccurs="0" nillable="true"  />
      <xs:element name="agencyFee" type="xs:float" minOccurs="0" nillable="true"  />
      <xs:element name="annualPercentageReturn" type="xs:decimal" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="pricePeriodicityId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="currency">
    <xs:restriction base="xs:string">
      <xs:enumeration value="EUR" />
      <xs:enumeration value="USD" />
      <xs:enumeration value="CHF" />
      <xs:enumeration value="GBP" />
      <xs:enumeration value="CAD" />
      <xs:enumeration value="NOK" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="priceTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="3" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="outdoorDescription">
    <xs:sequence>
      <xs:element name="nrOfFrontages" type="xs:int" minOccurs="0" />
      <xs:element name="frontageWidth" type="xs:int" minOccurs="0" />
      <xs:element name="orientationId" type="orientationId" minOccurs="0" />
      <xs:element name="nrOfFloors" type="xs:int" minOccurs="0" />
      <xs:element name="floorNumber" type="xs:int" minOccurs="0" />
      <xs:element name="nrOfHousings" type="xs:int" minOccurs="0" />
      <xs:element name="nrOfOutdoorParkings" type="xs:int" minOccurs="0" />
      <xs:element name="hasBalcony" type="xs:boolean" nillable="true" minOccurs="0" />
      <xs:element name="ground" type="outdoorDescriptionGround" minOccurs="0" maxOccurs="1" />
      <xs:element name="gardenSurface" type="xs:int" minOccurs="0" />
      <xs:element name="terraceSurface" type="xs:int" minOccurs="0" />
      <xs:element name="courtyardSurface" type="xs:int" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="orientationId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="8" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="outdoorDescriptionGround">
    <xs:sequence>
      <xs:element name="totalBuiltSurface" type="xs:decimal" minOccurs="0" />
      <xs:element name="surface" type="xs:decimal" minOccurs="0" />
      <xs:element name="depth" type="xs:decimal" minOccurs="0" />
      <xs:element name="frontWidth" type="xs:decimal" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="indoorDescription">
    <xs:sequence>
      <xs:element name="livableSurface" type="xs:decimal" nillable="true" minOccurs="0"  />
      <xs:element name="isFurnished" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="underCeilingHeight" type="xs:decimal"  nillable="true" minOccurs="0" />
      <xs:element name="isOpportunityForPro" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="floorCoveringTypeId" type="floorCoveringTypeId" minOccurs="0" />
      <xs:element name="glazingTypeId" type="glazingTypeId" minOccurs="0" />
      <xs:element name="hasDisabledAccess" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="hasElevator" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="windowFramingType" type="xs:string" minOccurs="0" />
      <xs:element name="rooms" type="indoorDescriptionRooms" minOccurs="0" maxOccurs="1" />
      <xs:element name="connections" type="indoorDescriptionConnections" minOccurs="0" maxOccurs="1" />
      <xs:element name="blindsTypeId" type="xs:int" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="floorCoveringTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="glazingTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="3" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="blindsTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="5" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="indoorDescriptionRooms">
    <xs:sequence>
      <xs:element name="bedroom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="livingRoom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="kitchen" type="indoorDescriptionKitchen" minOccurs="0" maxOccurs="1" />
      <!--<xs:element name="houseOffice" type="room" minOccurs="0" maxOccurs="1" />-->
      <xs:element name="bathroom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="shower" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="toilet" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="garage" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="interiorParking" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="cellar" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="dressingRoom" type="room" minOccurs="0" maxOccurs="1" />
	  <xs:element name="diningRoom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="attic" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="office" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="veranda" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="mezzanine" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="gameRoom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="entryHall" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="nightHall" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="washHouse" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="cloakroom" type="room" minOccurs="0" maxOccurs="1" />
      <xs:element name="bikeStorage" type="room" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
    <xs:attribute name="quantity" type="xs:int" />
  </xs:complexType>

  <xs:complexType name="room">
    <xs:sequence>
      <xs:element name="surface" type="xs:decimal" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="quantity" type="xs:int"/>
  </xs:complexType>

  <xs:complexType name="indoorDescriptionKitchen">
    <xs:sequence>
      <xs:element name="typeId" type="kitchenTypeId" />
      <xs:element name="typeId2" type="realKitchenTypeId" minOccurs="0" maxOccurs="1" />
      <xs:element name="surface" type="xs:decimal" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <!-- This is in fact the kitchenEquipmentTypeId. We should leave it like this, as it will impact the Pros -->
  <xs:simpleType name="kitchenTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>

  <!-- The real kitchenTypeId, used in the Wizard V2 -->
  <xs:simpleType name="realKitchenTypeId">
    <xs:restriction base="xs:int">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="4" />
    </xs:restriction>
  </xs:simpleType>
    
  <xs:complexType name="indoorDescriptionConnections">
    <xs:sequence>
      <xs:element name="hasSatelliteTv" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="hasCableTv" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="hasTelephone" type="xs:boolean" nillable="true" minOccurs="0"  />
      <xs:element name="hasWifi" type="xs:boolean" nillable="true" minOccurs="0"/>
      <xs:element name="hasWaterSupply" type="xs:boolean" nillable="true" minOccurs="0"  />
